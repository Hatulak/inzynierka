<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="628.0" prefWidth="827.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MainWindowController">
    <children>
        <MenuBar layoutY="2.0" prefHeight="22.0" prefWidth="827.0" AnchorPane.leftAnchor="0.0"
                 AnchorPane.rightAnchor="0.0">
            <menus>
                <Menu mnemonicParsing="false" text="%menubar.file">
                    <items>
                        <MenuItem fx:id="menuItemNewExperiment" mnemonicParsing="false"
                                  onAction="#openNewExperimentWindow" text="%menubar.new.experiment"/>
                        <MenuItem fx:id="menuItemClose" mnemonicParsing="false" text="%menubar.close"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <Label layoutX="49.0" layoutY="27.0" prefHeight="48.0" prefWidth="283.0" text="%main.window.experiments.list"
               AnchorPane.leftAnchor="49.0" AnchorPane.rightAnchor="495.0">
            <font>
                <Font size="20.0"/>
            </font>
        </Label>
        <ScrollPane layoutX="49.0" layoutY="75.0" prefHeight="553.0" prefWidth="721.0" AnchorPane.leftAnchor="49.0"
                    AnchorPane.rightAnchor="57.0">
            <content>
                <TableView fx:id="tableExperiments" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                           prefHeight="554.0" prefWidth="721.0">
                    <columns>
                        <TableColumn fx:id="tableColumnExperimentId" prefWidth="75.0" text="%experiment.id"/>
                        <TableColumn fx:id="tableColumnExperimentName" prefWidth="223.0" text="%experiment.name"/>
                        <TableColumn fx:id="tableColumnExperimentStatus" minWidth="5.0" prefWidth="116.0"
                                     text="%experiment.status"/>
                        <TableColumn fx:id="tableColumnExperimentAction" prefWidth="93.0" text="%experiment.action"/>
                        <TableColumn fx:id="tableColumnExperimentResults" prefWidth="108.0" text="%experiment.results"/>
                    </columns>
                </TableView>
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
